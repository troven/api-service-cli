{
  "name": "api-service-cli",
  "description": "API services",
  "version": "0.1.8",
  "author": "cto@troven.co",
  "bin": {
    "a6s": "./bin/run"
  },
  "bugs": "https://github.com/troven/api-service-cli/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^3",
    "@types/lodash": "^4.14.161",
    "@types/pouchdb": "^6.4.0",
    "@types/yamljs": "^0.2.31",
    "api-service-controller": "^0.13.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "pouchdb": "^7.2.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10.17.31",
    "globby": "^10",
    "json-loader": "^0.5.7",
    "source-map-support": "^0.5.10",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/troven/api-service-cli",
  "ignored": {
    "ts-loader": "^5.3.2"
  },
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "a6s",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "troven/api-service-cli",
  "scripts": {
    "build": "rm -rf lib && tsc -d",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "start": "tsc -d && node lib/start.js",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
